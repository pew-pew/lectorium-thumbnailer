{

  "manifest_version": 2,
  "name": "Thumbnailer",
  "version": "1.0",
 
  "description": "...",

  "applications": {
    "gecko": {
      "id": "fake@example.org"
    }
  },

  "web_accessible_resources": [
    "template.html",
    "spinner.gif"
  ],

  "content_scripts": [
    {
      "matches": ["*://*.youtube.com/playlist*"],
      "js": ["dist/vue.js", "dist/lodash.min.js", "main.js"],
      "css": ["styles.css", "spinner.css"]
    }
  ],

  "permissions": [
    "*://localhost/"
  ]
}